#!/usr/bin/env ruby

require "securerandom"
require_relative "client"

sink_event(SecureRandom.uuid)
sink_event(SecureRandom.uuid)
pp after_events(0)

puts "-" * 80

aggregate_id = SecureRandom.uuid
sink_event(aggregate_id)
sink_event(aggregate_id, aggregate_sequence: 2)
pp aggregate_events(aggregate_id)
